set -o vi
export TERM='xterm-256color'
alias mlnv='source {$HOME}/vlnv/bin/activate'
source $HOME/vlnv/bin/activate
PYTHONDONTWRITEBYTECODE=1
export PYTHONDONTWRITEBYTECODE=1
